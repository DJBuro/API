<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net">
    <object name="GPS" type="GeoCode.Service, GeoCode">
        <!--<property name="CoordinatesDao" ref="CoordinatesDao" />-->
        <property name="PostCodeDao" ref="PostCodeDao" />
    </object>

    <object id="GeoCode" type="Spring.Web.Services.WebServiceExporter, Spring.Web">
        <property name="TargetName" value="GPS"/>
        <property name="Namespace" value="http://gps.andromedagps.com"/>
        <property name="Description" value="Defines operations"/>
        <property name="MemberAttributes">
            <dictionary>
                <entry key="GeoCodeAddress">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="GeoCode an address"/>
                    </object>
                </entry>
            </dictionary>
        </property>
    </object>


</objects>