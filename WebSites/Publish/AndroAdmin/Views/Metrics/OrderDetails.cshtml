@model  DataWarehouseDataAccess.Domain.OrderHeaderDAO
@{
    ViewBag.Title = "ACS Order Details";
    Layout = null;
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ACS Order</title>
    <style type="text/css">
        table.gridtable {
            font-family: verdana,arial,sans-serif;
            font-size: 11px;
            color: #333333;
            border-width: 1px;
            border-color: #666666;
            border-collapse: collapse;
            width: 500px;
        }

            table.gridtable th {
                border-width: 1px;
                padding: 3px;
                border-style: solid;
                border-color: #666666;
                background-color: #dedede;
            }

            table.gridtable td {
                border-width: 1px;
                padding: 3px;
                border-style: solid;
                border-color: #666666;
                background-color: #dedede;
                /*background-color: #ffffff;*/
            }
    </style>
</head>
<body>
    <div>
        <div id="popupContent">
            <div class="editor-field">
                <div style="padding:5px">
                    <label><b>Partner Order#: </b></label>
                    <label>@Model.ACSOrderId</label>
                </div>
                <div style="padding:5px">
                    <label><b>Rameses Order#: </b></label>
                    <label>@Model.RamesesOrderNum</label>
                </div>
                <div style="padding:5px">
                    <label><b> ACS Application:</b></label>
                    <label>@Model.ApplicationName</label>
                </div>
                <div style="padding:5px">
                    <label><b> Store Name:</b></label>
                    <label>@Model.SiteName</label>
                </div>
            </div>
            <div class="editor-field">
                <div style="padding:5px">
                    <label><b>ACS Response: </b></label>
                    @if (@Model.ACSErrorCodeNumber != 0)
                    {
                        <b><label>@Model.ACSErrorCode.ShortDescription</label></b>
                    }
                    else
                    {
                        <b><label>Successful</label></b>
                    }
                </div>
                <div style="padding:5px">
                    <label><b> ACS Server:</b></label>
                    <label>@Model.ACSServer</label>
                    <label></label>
                </div>

                <div style="padding:5px">
                    <label><b> Status:</b></label>
                    @if (@Model.Status == 6)
                    {
                        <b><label>@Model.OrderStatus.Description</label></b>
                    }
                    else
                    {
                        <b><label>@Model.OrderStatus.Description</label></b>
                    }
                </div>
            </div>
        </div>
        <div style="height:100px">
        </div>

        <div id="popupContent">
            <div style="overflow-y:auto;height:auto;">
                <div style="overflow:auto">
                    <label style="font-size:medium"><b>Order Details</b></label>
                    <hr />
                    <div class="editor-field">
                        <div>
                            <label><b>Order Wanted Time:</b></label>
                            <label>@if (@Model.OrderWantedTime != null)
                            { @Model.OrderWantedTime.Value.ToString("dd-MMM-yyyy hh:mm:ss") } </label>
                        </div>
                        <div>
                            <label><b>Order Placed Time:</b></label>
                            <label>@if (@Model.OrderPlacedTime != null) { @Model.OrderPlacedTime.Value.ToString("dd-MMM-yyyy hh:mm:ss") } </label>
                        </div>
                        <div>
                            <label><b>Order Type:</b></label>
                            <label>@Model.OrderType</label>
                        </div>
                        <div>
                            <label><b> Order Currency:</b></label>
                            <label>@Model.OrderCurrency</label>
                        </div>
                        <div style="height:10px"></div>
                        <table class="gridtable">
                            <tr><th>Item No</th><th>Product</th><th>Qty</th><th>Price</th></tr>
                            @for (int i = 1; i <= @Model.OrderLines.Count; i++)
                            {
                                <tr>
                                    <td>@i</td>
                                    <td>@Model.OrderLines.ElementAt(i - 1).Description</td>
                                    <td>@Model.OrderLines.ElementAt(i - 1).Qty</td>
                                    <td>@String.Format("{0:C}", @Model.OrderLines.ElementAt(i - 1).Price)</td>
                                </tr>
                            }
                        </table>
                        @*@for (int i = 1; i <= @Model.OrderLines.Count; i++)
                            {
                                <div><b><u>Item @i</u></b></div>
                                <div style="padding:3px 3px 3px 10px">
                                    <label><b>Product Id: </b></label>
                                    <label>@Model.OrderLines.ElementAt(i - 1).ProductID</label>
                                </div>
                                <div style="padding:3px 3px 3px 10px">
                                    <label><b>Quantity:</b></label>
                                    <label>@Model.OrderLines.ElementAt(i - 1).Qty</label>
                                </div>
                                <div style="padding:3px 3px 3px 10px">
                                    <label><b>Price:</b></label>
                                    <label>@Model.OrderLines.ElementAt(i - 1).Price</label>
                                </div>

                                <div style="padding:3px 3px 3px 10px">
                                    <label><b>Description:</b></label>
                                    <label>@Model.OrderLines.ElementAt(i - 1).Description </label>
                                </div>
                            }*@


                    </div>

                </div>
                <div style="height:20px;"></div>
                <div style="overflow:auto">
                    <b>Customer Details</b><hr />
                    <div>
                        <div>
                            <label><b>Title:</b></label>
                            <label>@Model.Customer.Title</label>
                        </div>

                        <div>
                            <label><b>First Name:</b></label>
                            <label>@Model.Customer.FirstName</label>
                        </div>

                        <div>
                            <label><b>Surname:</b></label>
                            <label>@Model.Customer.LastName</label>
                        </div>
                        @if (@Model.CustomerAddress != null)
                        {
                            <div style="height:10px"></div>
                            <address>
                                <div>
                                    <b>Address</b>
                                </div>
                                <div style="padding-left:10px">
                                    <label><b>Road Name:</b></label>
                                    <label>@Model.CustomerAddress.RoadName </label>
                                </div>
                                <div style="padding-left:10px">
                                    <label><b>Road Number:</b></label>
                                    <label> @Model.CustomerAddress.RoadNum </label>
                                </div>
                                <div style="padding-left:10px">
                                    <label><b>City:</b></label>
                                    <label>@Model.CustomerAddress.City</label>
                                </div>
                                <div style="padding-left:10px">
                                    <label><b>State:</b></label>
                                    <label>@Model.CustomerAddress.State</label>
                                </div>
                                <div style="padding-left:10px">
                                    <label><b>Country:</b></label>
                                    <label>@Model.CustomerAddress.Country</label>
                                </div>
                                <div style="padding-left:10px">
                                    <label><b>Zipcode:</b></label>
                                    <label>@Model.CustomerAddress.ZipCode</label>
                                </div>
                            </address>
                        }
                        <div style="height:10px"></div>
                        @if (@Model.UsedVouchers != null && @Model.UsedVouchers.Count > 0)
                        {
                            <div>
                                <label><b>Voucher(s)</b></label><hr />
                                <table class="gridtable">
                                    <tr><th>Voucher</th><th>Used Count</th><th>Discount Type</th><th>Discount</th></tr>
                                    @for (int i = 0; i < @Model.UsedVouchers.Count; i++)
                                    {
                                        <tr>
                                            <td>@Model.UsedVouchers.ElementAt(i).Voucher.Code</td>
                                            <td>@Model.UsedVouchers.ElementAt(i).Voucher.MaxRepetitions</td>
                                            <td>@Model.UsedVouchers.ElementAt(i).Voucher.DiscountType</td>
                                            <td>@Model.UsedVouchers.ElementAt(i).Voucher.DiscountValue</td>
                                        </tr>
                                    }
                                </table>
                            </div>
                        }
                        <div style="height:10px"></div>
                        <div style="overflow:auto"><b>Pricing Details</b></div>
                        <hr />

                        <div>
                            <label><b>Pay Type:</b></label>
                            <label><b>@Model.paytype</b></label>
                        </div>
                        <div>
                            <label><b>Delivery Charge:</b></label>
                            <label><b>@String.Format("{0:C}", @Model.DeliveryCharge)</b></label>
                        </div>
                        <div>
                            <label><b>Total Tax:</b></label>
                            <label><b>@String.Format("{0:C}", @Model.TotalTax) </b></label>
                        </div>
                        <div>
                            <label><b>Price Include Tax:</b></label>
                            @if (@Model.PriceIncludeTax)
                            {
                                <label>Yes</label>
                            }
                            else
                            {
                                <label>No</label>
                            }
                        </div>
                        <div style="float:right">
                            <label><b>Final Price:</b></label>
                            <label style="font-size:large"><b>@String.Format("{0:C}", @Model.FinalPrice)</b></label>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
