/* BUTTON */
.highlightedButton a, .button a
{
    background-color: #7a9609;
    color: #74098f;
}
.sectionHeader a:link, .sectionHeader a:visited, .sectionHeader a:active
{
    background-color: #7a9609;
    color: #74098f;
}
.itemName, .dealName, .siteListCollectFromHere, #storeDetailsDeliveryTimes
{
    background-color: #7a9609 !IMPORTANT;
    color: #74098f !IMPORTANT;
}

/* SELECTED BUTTON */
.selectedSectionHeader a:link, .selectedSectionHeader a:visited, .selectedSectionHeader a:active
{
    background-color: #0e7ea0;
    color: #96a309;
}

/* MAIN MENU BAR */
#mainMenu
{
    background-color: #be1349;
}

/* MAIN MENU TEXT */
.mainMenuButtonSelected a:link, .mainMenuButtonSelected a:visited, .mainMenuButtonSelected a:active
{
    color: #39129d;
}

/* MAIN MENU SELECTED TEXT */
.mainMenuButton a:link, .mainMenuButton a:visited, .mainMenuButton a:active, .mainMenuButtonHighlighted a:link, .mainMenuButtonHighlighted a:visited, .mainMenuButtonHighlighted a:active
{
    color: <<MAINMENUSELECTEDTEXTCOLOUR>>;
}
