{
    width: auto !important;
    width: initial !IMPORTANT;
}

@media only screen and (max-width: 600px)
{
  .itemName 
  {
    min-width: initial;
    padding: 5px 10px;
    background-color: #a7b321;
    color: #ffffff;
    padding: 5px;
    left: 5px;
    position: absolute;
    display: inline-block;
    right: 80px;
    margin: 0;
  }
  .itemImage 
  {
    float: left;
    position: initial;
    margin: 0;
  }
  .itemDescription 
  {
    clear: initial;
    float: initial;
    margin-top: 40px;
  }

  .itemDetails, .itemDetailsWithImage 
  {
    float: none;
    margin: 0;
    position: absolute;
    top: 0;
    right: 0;
  }

  .priceAddContainer 
  {
    margin: 0;
    float: none;
  }

  .itemImage img 
  {
    margin: 35px 10px 0 0;
  }

  .itemRow
  {
    padding: 5px 0 5px 5px;
  }

}

.moName
{
  clear: both;
}
.moPriceTitle
{
  float:right;
}

