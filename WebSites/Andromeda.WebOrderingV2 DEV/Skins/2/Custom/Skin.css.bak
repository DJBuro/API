#checkoutSColumn1 > div, #checkoutSColumn2 > div { clear:both; padding:8px 0px; }

#checkoutSOrderItems {}
#checkoutSOrderItems div {width:150px;}
#checkoutSOrderItems .checkoutSOrderItem {width:auto!important; clear:both;}
#checkoutSOrderItems .checkoutSOrderItem .checkoutSOrderItemName {padding-right:10px;}
#checkoutSTotalPrompt, #checkoutSAddVoucherTitle {float:left; width:146px;}
#checkoutSTotalValue {display:inline-block;}
#checkoutSAddVoucherContainer > input, #checkoutSAddVoucherContainer > label {float:left; margin-right:10px;}
#checkoutSVoucherCode{ border: 1px solid #ccc; padding: 5px 4px;  margin-bottom:11px;}
#checkoutSAddressContainer > div, #checkoutSTime > div, #checkoutSPaymentSelection > div {float:left; width:146px;}
#checkoutSAddress {width:auto;}
#orderAccepted h3, #orderAccepted h4 {font-size:22px; line-height:25px;}
#orderAccepted {padding:10px 0px;}
.checkoutSOrderItemName {width:140px; float:left;}

/*forgot password*/
#forgotPasswordMessage {text-align:center; color:#ff0000;}
#forgotPasswordBox h1 {font-size:29px;}

#headerMyProfileButton a:link, #headerMyProfileButton a:visited, #headerMyProfileButton a:active, #headerMyOrdersButton a:link, #headerMyOrdersButton a:visited, #headerMyOrdersButton a:active, #headerLogoutButton a:link, #headerLogoutButton a:visited, #headerLogoutButton a:active {padding:15px 8px; /*font-size:12pt;*/ background-color:transparent; border:0px; }

/*header menu*/
#headerLoggedIn {position:relative; right:0px;}
#headerMyProfileButton, #headerMyOrdersButton, #headerLogoutButton {float:left;}
#headerLogoutButton, #headerMyOrdersButton {display:block;}

.popupToppingsSingle2 {margin-left:180px;}
.popupToppingsDouble, .popupToppingsDouble2 {margin-right:00px;}

/*footer pages*/
#informationInner {background-color:#ffffff; top:0px; width:100%; padding:20px; box-sizing:border-box;}

/*social media*/
#footerFacebook, #footerTwitter, #footerInstagram, #footerPInterest {display:inline-block; width:32px; height:32px; padding-right:10px;}
#footerSocialButtons {padding-top:10px;}
#footerTop {height:auto;}
