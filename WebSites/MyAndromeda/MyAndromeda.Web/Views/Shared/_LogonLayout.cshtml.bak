
@helper RenderFooter()
{
    @Html.Partial("_Footer")    
}


<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/styles/myAndromeda/css")

    @Scripts.Render("~/bundles/modernizr")
    
@*    @Scripts.Render("~/bundles/jquery")*@
    
	<script src="~/Scripts/kendo.modernizr.custom.js")></script>
	<link href="http://cdn.kendostatic.com/2013.2.716/styles/kendo.common.min.css" rel="stylesheet" type="text/css" />
	<link href="http://cdn.kendostatic.com/2013.2.716/styles/kendo.dataviz.min.css" rel="stylesheet" type="text/css" />
	<link href="http://cdn.kendostatic.com/2013.2.716/styles/kendo.bootstrap.min.css" rel="stylesheet" type="text/css" />
	<link href="http://cdn.kendostatic.com/2013.2.716/styles/kendo.dataviz.bootstrap.min.css" rel="stylesheet" type="text/css" />
	<script src="http://cdn.kendostatic.com/2013.2.716/js/jquery.min.js"></script>
	<script src="http://cdn.kendostatic.com/2013.2.716/js/kendo.all.min.js"></script>
	<script src="http://cdn.kendostatic.com/2013.2.716/js/kendo.aspnetmvc.min.js"></script>
</head>
<body>
    <!--Header Starts-->
    <div id="header">
        <div id="header-inner">
            <!--start logo-->
            <div id="logo">
                <img src="@Url.Content("~/Content/Images/logo.png")" alt="My Andromeda"/>
                <div id="title">
                    <h1><a href="">My Andromeda</a></h1>
                    <p>A World of epossibilities</p>
                </div>
            </div>
            <!-- end logo -->
            <div class="clear"></div>
        </div><!-- end header-inner -->
    </div>
    <!--Header Ends-->

    <!--Start Body -->
    <div class="container">	
        @if (Html.HasNotificationMessages()) { 
            <section id="notifications" class="k-block k-info-colored">
            @foreach (var message in Html.NotificationMessages()) 
            {
                <div class="message">@message</div>
            }
            </section>
            <div class="separator"></div>
        }
        <section id="main">
            @RenderBody()
        </section>
    </div>
    @RenderFooter()

    @RenderSection("scripts", required: false)

    @Html.PageScripts()
</body>
</html>
