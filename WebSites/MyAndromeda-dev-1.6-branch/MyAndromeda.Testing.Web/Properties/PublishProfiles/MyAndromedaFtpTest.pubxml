          <Object Type="DbDacFx">
            <PreSource Path="Provider=Microsoft.Jet.OLEDB.4.0;Data Source=|DataDirectory|\Menus\{0}\menu.mdb;Jet OLEDB:Database Password=zangaboo" includeData="False" />
