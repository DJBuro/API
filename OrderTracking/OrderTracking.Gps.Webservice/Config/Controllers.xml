<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net">
    <object name="GPS" type="OrderTracking.Gps.Webservice.Service, OrderTracking.Gps.Webservice">
        <property name="DeviceDao" ref="DeviceDao" />
        <property name="UserDao" ref="UserDao" />
        <property name="GateUserDao" ref="GateUserDao" />
        <property name="UserGroupDao" ref="UserGroupDao" />
        <property name="LicenseDao" ref="LicenseDao" />
    </object>

    <object id="Trackers" type="Spring.Web.Services.WebServiceExporter, Spring.Web">
        <property name="TargetName" value="GPS"/>
        <property name="Namespace" value="http://gps.andromedagps.com"/>
        <property name="Description" value="Defines operations"/>
        <property name="MemberAttributes">
            <dictionary>
                <entry key="AddTracker">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Adds a new tracker"/>
                    </object>
                </entry>                
                <entry key="GetLicense">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Gets license info"/>
                    </object>
                </entry>
                <entry key="GetTrackerByName">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Gets the current details of a tracker"/>
                    </object>
                </entry>
                <entry key="GetTrackersByNames">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Gets the current details of a list of trackers"/>
                    </object>
                </entry>
                <entry key="UpdateTrackerPhoneNumber">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Update a tracker phone number"/>
                    </object>
                </entry>
            </dictionary>
        </property>
    </object>


</objects>