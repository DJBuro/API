
<objects xmlns="http://www.springframework.net">

<v:group id="OrderValidator" xmlns:v="http://www.springframework.net/validation">


	<v:required id="ExternalOrderIdValidator" test="ExternalOrderId"><v:message id="error.ExternalOrderId.required" providers="ExternalOrderIdErrors, ValidationSummary"/></v:required>
	<v:required id="NameValidator" test="Name"><v:message id="error.Name.required" providers="NameErrors, ValidationSummary"/></v:required>
	<v:required id="CreatedValidator" test="Created"><v:message id="error.Created.required" providers="CreatedErrors, ValidationSummary"/></v:required>
</v:group>

</objects>




			