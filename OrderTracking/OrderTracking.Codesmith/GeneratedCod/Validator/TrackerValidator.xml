
<objects xmlns="http://www.springframework.net">

<v:group id="TrackerValidator" xmlns:v="http://www.springframework.net/validation">


	<v:required id="NameValidator" test="Name"><v:message id="error.Name.required" providers="NameErrors, ValidationSummary"/></v:required>
	<v:required id="IMEIValidator" test="IMEI"><v:message id="error.IMEI.required" providers="IMEIErrors, ValidationSummary"/></v:required>
	<v:required id="PhoneNumberValidator" test="PhoneNumber"><v:message id="error.PhoneNumber.required" providers="PhoneNumberErrors, ValidationSummary"/></v:required>
	<v:required id="ActiveValidator" test="Active"><v:message id="error.Active.required" providers="ActiveErrors, ValidationSummary"/></v:required>
</v:group>

</objects>




			