
<objects xmlns="http://www.springframework.net">

<v:group id="SmsCredentialValidator" xmlns:v="http://www.springframework.net/validation">


	<v:required id="UsernameValidator" test="Username"><v:message id="error.Username.required" providers="UsernameErrors, ValidationSummary"/></v:required>
	<v:required id="PasswordValidator" test="Password"><v:message id="error.Password.required" providers="PasswordErrors, ValidationSummary"/></v:required>
	<v:required id="SmsFromValidator" test="SmsFrom"><v:message id="error.SmsFrom.required" providers="SmsFromErrors, ValidationSummary"/></v:required>
</v:group>

</objects>




			