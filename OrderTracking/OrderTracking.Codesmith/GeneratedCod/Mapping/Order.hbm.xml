<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
	<class name="OrderTracking.Dao.Domain.Order, OrderTracking.Dao" table="tbl_Order">
		<id name="Id" type="Int64" unsaved-value="null">
			<column name="id" length="8" sql-type="bigint" not-null="true" unique="true" index="PK_tbl_Order"/>
			<generator class="native" />
		</id>
		<property name="ExternalOrderId" type="String">
			<column name="ExternalOrderId" length="50" sql-type="nvarchar" not-null="true"/>
		</property>
		<property name="Name" type="String">
			<column name="Name" length="50" sql-type="nvarchar" not-null="true"/>
		</property>
		<property name="TicketNumber" type="String">
			<column name="TicketNumber" length="50" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="ExtraInformation" type="String">
			<column name="ExtraInformation" length="100" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="ProximityDelivered" type="DateTime">
			<column name="ProximityDelivered" length="4" sql-type="smalldatetime" not-null="false"/>
		</property>
		<property name="Created" type="DateTime">
			<column name="Created" length="8" sql-type="datetime" not-null="true"/>
		</property>
		<many-to-one name="StoreId" class="OrderTracking.Dao.Domain.Store, OrderTracking.Dao">
			<column name="StoreId" length="8" sql-type="bigint" not-null="true"/>
		</many-to-one>
		<bag name="OrderIdOrderStatuses" inverse="true" lazy="true" cascade="all-delete-orphan">
			<key column="OrderId"/>
			<one-to-many class="OrderTracking.Dao.Domain.OrderStatus, OrderTracking.Dao"/>
		</bag>
		<bag name="OrderIdItems" inverse="true" lazy="true" cascade="all-delete-orphan">
			<key column="OrderId"/>
			<one-to-many class="OrderTracking.Dao.Domain.Item, OrderTracking.Dao"/>
		</bag>
		<bag name="OrderIdCustomerOrderses" inverse="true" lazy="true" cascade="all-delete-orphan">
			<key column="OrderId"/>
			<one-to-many class="OrderTracking.Dao.Domain.CustomerOrder, OrderTracking.Dao"/>
		</bag>
		<bag name="OrderIdDriverOrderses" inverse="true" lazy="true" cascade="all-delete-orphan">
			<key column="OrderId"/>
			<one-to-many class="OrderTracking.Dao.Domain.DriverOrder, OrderTracking.Dao"/>
		</bag>
	</class>
</hibernate-mapping>



