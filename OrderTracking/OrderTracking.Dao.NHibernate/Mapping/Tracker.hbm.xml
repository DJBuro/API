<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
	<class name="OrderTracking.Dao.Domain.Tracker, OrderTracking.Dao" table="tbl_Tracker" batch-size="100">
		<id name="Id" type="Int64" unsaved-value="null">
			<column name="id" length="8" sql-type="bigint" not-null="true" unique="true" index="PK_tbl_Tracker"/>
			<generator class="native" />
		</id>
		<property name="Name" type="String">
			<column name="Name" length="50" sql-type="nvarchar" not-null="true" unique="true" index="IX_tbl_TrackerName"/>
		</property>
		<property name="BatteryLevel" type="Int32">
			<column name="BatteryLevel" length="4" sql-type="int" not-null="false"/>
		</property>
		<property name="IMEI" type="String">
			<column name="IMEI" length="50" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="SerialNumber" type="String">
			<column name="SerialNumber" length="50" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="PhoneNumber" type="String">
			<column name="PhoneNumber" length="50" sql-type="nvarchar" not-null="true" unique="true" index="IX_tbl_TrackerPhoneNumber"/>
		</property>
		<property name="Active" type="Boolean">
			<column name="Active" length="1" sql-type="bit" not-null="true"/>
		</property>
    <property name="LastUpdate" type="DateTime">
      <column name="LastUpdate" sql-type="datetime" not-null="false" />
    </property>
		<many-to-one name="Coordinates" class="OrderTracking.Dao.Domain.Coordinates, OrderTracking.Dao">
			<column name="Coordinates" length="8" sql-type="bigint" not-null="false"/>
		</many-to-one>
		<many-to-one name="Status" class="OrderTracking.Dao.Domain.TrackerStatus, OrderTracking.Dao">
			<column name="Status" length="8" sql-type="bigint" not-null="true"/>
		</many-to-one>
		<many-to-one name="Store" class="OrderTracking.Dao.Domain.Store, OrderTracking.Dao">
			<column name="StoreId" length="8" sql-type="bigint" not-null="false"/>
		</many-to-one>
		<many-to-one name="Driver" class="OrderTracking.Dao.Domain.Driver, OrderTracking.Dao">
			<column name="DriverId" length="8" sql-type="bigint" not-null="false"/>
		</many-to-one>
		<many-to-one name="Apn" class="OrderTracking.Dao.Domain.Apn, OrderTracking.Dao">
			<column name="ApnId" length="8" sql-type="bigint" not-null="false"/>
		</many-to-one>
		<many-to-one name="Type" class="OrderTracking.Dao.Domain.TrackerType, OrderTracking.Dao">
			<column name="TypeId" length="8" sql-type="bigint" not-null="true"/>
		</many-to-one>
	</class>
</hibernate-mapping>



