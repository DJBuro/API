<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
	<class name="OrderTracking.Dao.Domain.OrderStatus, OrderTracking.Dao" table="tbl_OrderStatus">
        <cache usage="read-write" />
		<id name="Id" type="Int64" unsaved-value="null">
			<column name="id" length="8" sql-type="bigint" not-null="true" unique="true" index="PK_tbl_OrderStatus"/>
			<generator class="native" />
		</id>
		<property name="Processor" type="String">
			<column name="Processor" length="50" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="Time" type="DateTime">
			<column name="`Time`" length="4" sql-type="smalldatetime" not-null="false"/>
		</property>
		<many-to-one name="Status" class="OrderTracking.Dao.Domain.Status, OrderTracking.Dao" fetch="join">
			<column name="StatusId" length="8" sql-type="bigint" not-null="true"/>
		</many-to-one>
		<many-to-one name="Order" class="OrderTracking.Dao.Domain.Order, OrderTracking.Dao" fetch="join">
			<column name="OrderId" length="8" sql-type="bigint" not-null="true"/>
		</many-to-one>
    <property name="CreatedDateTime" type="DateTime">
      <column name="CreatedDateTime" sql-type="datetime" not-null="false"/>
    </property>
    <property name="DispatchedDateTime" type="DateTime">
      <column name="DispatchedDateTime" sql-type="datetime" not-null="false"/>
    </property>
	</class>
</hibernate-mapping>



