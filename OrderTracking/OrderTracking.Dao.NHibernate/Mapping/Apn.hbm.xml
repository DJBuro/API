<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
	<class name="OrderTracking.Dao.Domain.Apn, OrderTracking.Dao" table="tbl_Apn">
		<id name="Id" type="Int64" unsaved-value="null">
			<column name="id" length="8" sql-type="bigint" not-null="true" unique="true" index="PK_tbl_Apn"/>
			<generator class="native" />
		</id>
		<property name="Provider" type="String">
			<column name="Provider" length="50" sql-type="nvarchar" not-null="true"/>
		</property>
		<property name="Name" type="String">
			<column name="Name" length="50" sql-type="nvarchar" not-null="true"/>
		</property>
		<property name="Username" type="String">
			<column name="Username" length="50" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="Password" type="String">
			<column name="Password" length="50" sql-type="nvarchar" not-null="false"/>
		</property>
		<bag name="ApnTrackers" inverse="true" lazy="true" cascade="all-delete-orphan">
			<key column="ApnId"/>
			<one-to-many class="OrderTracking.Dao.Domain.Tracker, OrderTracking.Dao"/>
		</bag>
	</class>
</hibernate-mapping>



