<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
	<class name="OrderTracking.Dao.Domain.CustomerOrder, OrderTracking.Dao" table="tbl_CustomerOrders">
        <cache usage="read-write" />
		<id name="Id" type="Int64" unsaved-value="null">
			<column name="id" length="8" sql-type="bigint" not-null="true" unique="true" index="PK_tbl_CustomerOrders"/>
			<generator class="native" />
		</id>
		<many-to-one name="Order" class="OrderTracking.Dao.Domain.Order, OrderTracking.Dao" fetch="join">
			<column name="OrderId" length="8" sql-type="bigint" not-null="true"/>
		</many-to-one>
		<many-to-one name="Customer" class="OrderTracking.Dao.Domain.Customer, OrderTracking.Dao" fetch="join">
			<column name="CustomerId" length="8" sql-type="bigint" not-null="true"/>
		</many-to-one>
	</class>
</hibernate-mapping>



