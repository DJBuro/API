<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
	<class name="OrderTracking.Dao.Domain.Client, OrderTracking.Dao" table="Client" batch-size="100">
    <cache usage="read-write" />
		<id name="Id" type="Int64" unsaved-value="null">
			<column name="id" length="8" sql-type="bigint" not-null="true" unique="true" index="PK_Order"/>
			<generator class="native" />
		</id>
		<property name="Name" type="String">
			<column name="Name" length="255" sql-type="nvarchar" not-null="true"/>
		</property>
    <property name="WebKey" type="String">
      <column name="WebKey" length="255" sql-type="nvarchar" not-null="true"/>
    </property>
    <property name="WebsiteTemplateName" type="String">
      <column name="WebsiteTemplateName" length="255" sql-type="nvarchar" not-null="true"/>
    </property>
	</class>
</hibernate-mapping>



