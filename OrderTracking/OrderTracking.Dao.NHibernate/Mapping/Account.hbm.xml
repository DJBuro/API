<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
	<class name="OrderTracking.Dao.Domain.Account, OrderTracking.Dao" table="tbl_Account" batch-size="100">
      <cache usage="nonstrict-read-write"/>
      <id name="Id" type="Int64" unsaved-value="null">
      <column name="id" length="8" sql-type="bigint" not-null="true" unique="true" index="PK_tbl_Store"/>
      <generator class="native" />
    </id>
		<property name="UserName" type="String">
			<column name="UserName" length="20" sql-type="nvarchar" not-null="true"/>
		</property>
		<property name="Password" type="String">
			<column name="Password" length="20" sql-type="nvarchar" not-null="true"/>
		</property>
		<property name="GpsEnabled" type="Boolean">
			<column name="GpsEnabled" length="1" sql-type="bit" not-null="true"/>
		</property>
		<many-to-one name="Store" class="OrderTracking.Dao.Domain.Store, OrderTracking.Dao" fetch="join">
			<column name="StoreId" length="8" sql-type="bigint" not-null="true"/>
		</many-to-one>
	</class>
</hibernate-mapping>



