<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
	<class name="OrderTracking.Dao.Domain.Log, OrderTracking.Dao" table="tbl_Log">
        <cache usage="read-write" />
		<id name="Id" type="Int64" unsaved-value="null">
			<column name="id" length="8" sql-type="bigint" not-null="true" unique="true" index="PK_tbl_Log"/>
			<generator class="native" />
		</id>
		<property name="StoreId" type="String">
			<column name="StoreId" length="50" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="Severity" type="String">
			<column name="Severity" length="50" sql-type="nvarchar" not-null="true"/>
		</property>
		<property name="Message" type="String">
			<column name="Message" length="500" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="Method" type="String">
			<column name="Method" length="200" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="Source" type="String">
			<column name="Source" length="50" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="Created" type="DateTime">
			<column name="Created" length="8" sql-type="datetime" not-null="true"/>
		</property>
	</class>
</hibernate-mapping>



