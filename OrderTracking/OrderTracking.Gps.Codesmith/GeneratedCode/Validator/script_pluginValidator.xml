
<objects xmlns="http://www.springframework.net">

<v:group id="ScriptpluginValidator" xmlns:v="http://www.springframework.net/validation">


	<v:required id="NameValidator" test="Name"><v:message id="error.Name.required" providers="NameErrors, ValidationSummary"/></v:required>
	<v:required id="CategoryValidator" test="Category"><v:message id="error.Category.required" providers="CategoryErrors, ValidationSummary"/></v:required>
	<v:required id="FilepathValidator" test="Filepath"><v:message id="error.Filepath.required" providers="FilepathErrors, ValidationSummary"/></v:required>
	<v:required id="VersionValidator" test="Version"><v:message id="error.Version.required" providers="VersionErrors, ValidationSummary"/></v:required>
</v:group>

</objects>




			