
<objects xmlns="http://www.springframework.net">

<v:group id="SchedulertaskValidator" xmlns:v="http://www.springframework.net/validation">


	<v:required id="TasknameValidator" test="Taskname"><v:message id="error.Taskname.required" providers="TasknameErrors, ValidationSummary"/></v:required>
	<v:required id="ExeccountValidator" test="Execcount"><v:message id="error.Execcount.required" providers="ExeccountErrors, ValidationSummary"/></v:required>
	<v:required id="StateValidator" test="State"><v:message id="error.State.required" providers="StateErrors, ValidationSummary"/></v:required>
	<v:required id="TaskassemblynameValidator" test="Taskassemblyname"><v:message id="error.Taskassemblyname.required" providers="TaskassemblynameErrors, ValidationSummary"/></v:required>
	<v:required id="TasktypenameValidator" test="Tasktypename"><v:message id="error.Tasktypename.required" providers="TasktypenameErrors, ValidationSummary"/></v:required>
</v:group>

</objects>




			