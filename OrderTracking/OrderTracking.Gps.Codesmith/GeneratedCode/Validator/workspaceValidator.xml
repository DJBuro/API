
<objects xmlns="http://www.springframework.net">

<v:group id="WorkspaceValidator" xmlns:v="http://www.springframework.net/validation">


	<v:required id="NameValidator" test="Name"><v:message id="error.Name.required" providers="NameErrors, ValidationSummary"/></v:required>
	<v:required id="StateValidator" test="State"><v:message id="error.State.required" providers="StateErrors, ValidationSummary"/></v:required>
	<v:required id="SharedValidator" test="Shared"><v:message id="error.Shared.required" providers="SharedErrors, ValidationSummary"/></v:required>
</v:group>

</objects>




			