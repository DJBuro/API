
<objects xmlns="http://www.springframework.net">

<v:group id="GateeventValidator" xmlns:v="http://www.springframework.net/validation">


	<v:required id="UseridValidator" test="Userid"><v:message id="error.Userid.required" providers="UseridErrors, ValidationSummary"/></v:required>
	<v:required id="StageValidator" test="Stage"><v:message id="error.Stage.required" providers="StageErrors, ValidationSummary"/></v:required>
	<v:required id="ScheduledValidator" test="Scheduled"><v:message id="error.Scheduled.required" providers="ScheduledErrors, ValidationSummary"/></v:required>
</v:group>

</objects>




			