<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
	<class name="OrderTracking.Gps.Dao.Domain.Geocoder, OrderTracking.Gps.Dao" table="geocoder">
		<id name="Id" type="Int32" unsaved-value="null">
			<column name="geocoder_id" length="4" sql-type="int" not-null="true" unique="true" index="PK_geocoder"/>
			<generator class="native" />
		</id>
		<property name="Name" type="String">
			<column name="name" length="64" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="Description" type="String">
			<column name="description" length="128" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="Botype" type="String">
			<column name="bo_type" length="128" sql-type="varchar" not-null="true"/>
		</property>
		<one-to-one name="Geocoderapplication" class="OrderTracking.Gps.Dao.Domain.Geocoderapplication, OrderTracking.Gps.Dao"/>
		<bag name="geocoder_providers" inverse="true" lazy="true" cascade="all-delete-orphan">
			<key column="geocoder_id"/>
			<one-to-many class="OrderTracking.Gps.Dao.Domain.Geocoderprovider, OrderTracking.Gps.Dao"/>
		</bag>
	</class>
</hibernate-mapping>



