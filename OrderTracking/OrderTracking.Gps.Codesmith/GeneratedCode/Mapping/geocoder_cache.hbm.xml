<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
	<class name="OrderTracking.Gps.Dao.Domain.Geocodercache, OrderTracking.Gps.Dao" table="geocoder_cache">
		<id name="Id" type="Int32" unsaved-value="null">
			<column name="geocoder_cache_id" length="4" sql-type="int" not-null="true" unique="true" index="PK_geocoder_cache"/>
			<generator class="native" />
		</id>
		<property name="Lon" type="Double">
			<column name="lon" length="8" sql-type="float" not-null="true"/>
		</property>
		<property name="Lat" type="Double">
			<column name="lat" length="8" sql-type="float" not-null="true"/>
		</property>
		<property name="Countryname" type="String">
			<column name="country_name" length="128" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="Cityname" type="String">
			<column name="city_name" length="128" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="Postalcodenumber" type="String">
			<column name="postal_code_number" length="45" sql-type="varchar" not-null="false"/>
		</property>
		<property name="Streetname" type="String">
			<column name="street_name" length="128" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="Streetnumber" type="String">
			<column name="street_number" length="45" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="Streetbox" type="String">
			<column name="street_box" length="45" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="Address" type="String">
			<column name="address" length="128" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="Lonlathash" type="Int64">
			<column name="lon_lat_hash" length="8" sql-type="bigint" not-null="true"/>
		</property>
		<many-to-one name="Geocoderprovider" class="OrderTracking.Gps.Dao.Domain.Geocoderprovider, OrderTracking.Gps.Dao">
			<column name="geocoder_provider_id" length="4" sql-type="int" not-null="true"/>
		</many-to-one>
	</class>
</hibernate-mapping>



