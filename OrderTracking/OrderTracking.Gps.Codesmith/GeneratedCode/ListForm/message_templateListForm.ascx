<%@ Control Language="C#" AutoEventWireup="true" CodeBehind="MessagetemplateListForm.ascx.cs" Inherits="OrderTracking.Gps.Data.MessagetemplateListForm" %>
<Cacd:ValidationSummary ID="validationSummary" Provider="summary" runat="server" />
<asp:Repeater ID="dataItemsList" runat="server">
    <HeaderTemplate>
        <table border="0" width="100%" cellpadding="0" cellspacing="0" class="suggestedTable">
            <thead>
				<tr>
					GetMessage("Templatetext")
					GetMessage("Botype")
                </tr>
            </thead>
            <tbody>
    </HeaderTemplate>
    <ItemTemplate>
		<tr>
			<td>
                <\%# ((Messagetemplate) Container.DataItem).Templatetext\%>
            </td>
			<td>
                <\%# ((Messagetemplate) Container.DataItem).Botype\%>
            </td>
		</tr>
    </ItemTemplate>
    <FooterTemplate>
            </tbody></table>
    </FooterTemplate>
</asp:Repeater>




