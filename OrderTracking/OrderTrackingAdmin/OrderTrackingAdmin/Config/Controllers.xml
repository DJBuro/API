    <property name="ClientDao" ref="ClientDao" />
    <property name="ClientStoreDao" ref="ClientStoreDao" />
    <property name="ClientWebsiteCustomContentDao" ref="ClientWebsiteCustomContentDao" />
    <property name="OrderDao" ref="OrderDao" />
    <property name="DriverDao" ref="DriverDao" />
    <property name="OrderStatusDao" ref="OrderStatusDao" />
    <property name="CoordinatesDao" ref="CoordinatesDao" />
    <property name="CustomerDao" ref="CustomerDao" />
  </object>
  
  <object id="TrackersController" parent="SiteBaseController" type="OrderTrackingAdmin.Controllers.TrackersController" singleton="false">
        <property name="TrackerDao" ref="TrackerDao" />
        <property name="ApnDao" ref="ApnDao" />
        <property name="TrackerTypeDao" ref="TrackerTypeDao" />
        <property name="TrackerCommandDao" ref="TrackerCommandDao" />
        <property name="SmsCredentialDao" ref="SmsCredentialDao" />
    </object>

    <object id="GlobalController" parent="SiteBaseController" type="OrderTrackingAdmin.Controllers.GlobalController" singleton="false">
        <property name="ApnDao" ref="ApnDao" />
        <property name="TrackerTypeDao" ref="TrackerTypeDao" />
        <property name="LogDao" ref="LogDao" />
        <property name="SmsCredentialDao" ref="SmsCredentialDao" />
    </object>

