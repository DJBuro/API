<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net">

    <object id="SiteBaseController" abstract="true">
        <property name="AccountDao" ref="AccountDao" />
        <property name="StoreDao" ref="StoreDao" />
        <property name="LanguageCode" value="en" />
    </object>

    <object id="StoreController" parent="SiteBaseController" type="OrderTrackingAdmin.Controllers.StoreController" singleton="false">
        <property name="TrackerDao" ref="TrackerDao" />
        <property name="LogDao" ref="LogDao" />
        <property name="OrderDao" ref="OrderDao" />
        <property name="DriverDao" ref="DriverDao" />
        <property name="OrderStatusDao" ref="OrderStatusDao" />
        <property name="CoordinatesDao" ref="CoordinatesDao" />
    </object>    
    
    <object id="AccountController" parent="SiteBaseController" type="OrderTrackingAdmin.Controllers.AccountController" singleton="false">
    </object>
    
</objects>
