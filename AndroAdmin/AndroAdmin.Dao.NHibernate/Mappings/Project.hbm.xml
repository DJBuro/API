<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
	<class name="AndroAdmin.Dao.Domain.Project, AndroAdmin.Dao" table="tbl_Project">
		<id name="Id" type="Int32" unsaved-value="null">
			<column name="id" length="4" sql-type="int" not-null="true" unique="true" index="PK_tbl_Project"/>
			<generator class="native" />
		</id>
		<property name="Name" type="String">
			<column name="Name" length="20" sql-type="varchar" not-null="true"/>
		</property>
		<property name="Url" type="String">
			<column name="Url" length="200" sql-type="varchar" not-null="true"/>
		</property>
		<bag name="UserPermissions" inverse="true" lazy="true" cascade="all-delete-orphan">
			<key column="Project"/>
			<one-to-many class="AndroAdmin.Dao.Domain.AndroUserPermission, AndroAdmin.Dao"/>
		</bag>
	</class>
</hibernate-mapping>



