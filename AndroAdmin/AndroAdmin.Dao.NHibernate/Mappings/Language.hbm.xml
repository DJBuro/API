		</id>
		<property name="Name" type="String">
			<column name="Name" length="20" sql-type="nvarchar" not-null="true"/>
		</property>
		<property name="Code" type="String">
			<column name="Code" length="2" sql-type="nvarchar" not-null="true"/>
		</property>
