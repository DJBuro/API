									startAngle="{_GAUGESTART}" />
			<!-- Green arcs -->
			<geometry:EllipticalArc closureType="open" id="Gauge05GreenArc"
									arc="{-_gauge05GreenArc}" width="{_gaugeRadius*.89}"  height="{_gaugeRadius*.89}" x="{_gaugeRadius*.055}" y="{_gaugeRadius*.063}" 
									startAngle="{_gauge05GreenArcStart}" />
			
			
			<degrafa:GeometryRepeater count="{_gauge05MajorTickCount}">
				<degrafa:RegularRectangle height="{_fontSize *.5}"  width="2" x="{_gaugeRadius*.5}" y="{_gaugeRadius*.05}" stroke="{majorMarksStroke}">
					<degrafa:transform>
						<degrafa:RotateTransform 
							angle="{_gaugeAngle}"
							id="Rtransform05"
							centerX="{_gaugeHandLocation}" 
							centerY="{_gaugeHandLocation}"
							/>
					</degrafa:transform>
				</degrafa:RegularRectangle>
				<degrafa:modifiers>
					<degrafa:PropertyModifier  targets="{[Rtransform05]}" property="angle" modifier="{_gauge05MajorTickOffset}" modifierOperator="add" />
				</degrafa:modifiers>
			</degrafa:GeometryRepeater>
			
			<degrafa:GeometryRepeater count="{_gauge05MajorTickCount}">
				<degrafa:RasterText id="gauge05GaugeText"  align="left" fontSize="{_fontSize *.8}" textColor="#999999" fontWeight="bold" fontFamily="Arial" x="{_gaugeRadius*.5}" y="{_gaugeRadius*.1}">
					<degrafa:transform>
						<degrafa:TransformGroup>
							<degrafa:RotateTransform 
								angle="{_gaugeAngle - 5}"
								id="RtransformG05"
								centerX="{_gaugeHandLocation}" 
								centerY="{_gaugeHandLocation}"
								/>
							<!-- <degrafa:RotateTransform id="numberRotate" registrationPoint="center" /> -->
						</degrafa:TransformGroup>
					</degrafa:transform>
				</degrafa:RasterText>
				<degrafa:modifiers>
					<degrafa:PropertyModifier  targets="{[RtransformG05]}" property="angle" modifier="{_gauge05MajorTickOffset}" modifierOperator="add" />
					<!--<degrafa:PropertyModifier targets="{[numberRotate]}" property="angle"  modifier="{_gauge01MajorTickOffset-20}"  modifierOperator="add"/>-->
					<degrafa:PropertyModifier  targets="{[gauge05GaugeText]}" property="text" modifier="{gauge05TickLabels}" modifierOperator="none" />
				</degrafa:modifiers>
			</degrafa:GeometryRepeater>
			
		</degrafa:GeometryGroup>
		
		<!-- 05 hand -->        
		<degrafa:GeometryGroup id="gauge05HandDS">
			<!-- Indicator a Polygon Shadow. -->
