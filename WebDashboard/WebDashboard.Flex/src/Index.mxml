			<degrafa:RasterText id="gauge05LaggersText" fontSize="{_fontSize}" text="Laggers"  fontFamily="Arial" fontWeight="bold" textColor="#999999"/>
			<degrafa:RasterText id="gauge05Laggers1" fontSize="{_fontSize}" width="{_gaugeRadius}" textColor="#FFFFFF" fontFamily="Arial"  />
			<degrafa:RasterText id="gauge05Laggers2" fontSize="{_fontSize}" width="{_gaugeRadius}" textColor="#FFFFFF"  fontFamily="Arial" />
			<degrafa:RasterText id="gauge05Laggers3" fontSize="{_fontSize}" width="{_gaugeRadius}" textColor="#FFFFFF" fontFamily="Arial"  />
			
			<!-- Benchmark arcs -->
			<geometry:EllipticalArc closureType="open" id="Gauge05BenchmarkArc"
