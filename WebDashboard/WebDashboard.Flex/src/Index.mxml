			<degrafa:RasterText id="column06Leaders102" fontSize="{_fontSize}" width="{_gaugeRadius}" textColor="#FFFFFF"  fontFamily="Arial" />
			<degrafa:RasterText id="column06Leaders202" fontSize="{_fontSize}" width="{_gaugeRadius}" textColor="#FFFFFF"  fontFamily="Arial" />
			<degrafa:RasterText id="column06Leaders302" fontSize="{_fontSize}" width="{_gaugeRadius}" textColor="#FFFFFF"  fontFamily="Arial" />
