			<!-- Up/Down Arrow 02-->
			<degrafa:GeometryGroup id="column06ArrowDS02">
				<!-- Indicator a Polygon. -->
				<degrafa:Polygon id="column06ArrowIndicatorDS02" fill="{blackFill}"
								 data="{
								 String(_gaugeRadius*-.1) + ',' + String(_gaugeRadius*-.1) + ' ' + 
								 String(0) + ',' + String(_gaugeRadius*.1) + ' ' +
								 String(_gaugeRadius*.1) + ',' + String(_gaugeRadius*-.1)
