			<!-- Up/Down Arrow 01-->
			<degrafa:GeometryGroup id="column06ArrowDS01">
				<!-- Indicator a Polygon. -->
				<degrafa:Polygon id="column06ArrowIndicatorDS01" fill="{blackFill}"
								 data="{
								 String(_gaugeRadius*-.1) + ',' + String(_gaugeRadius*-.1) + ' ' + 
								 String(0) + ',' + String(_gaugeRadius*.1) + ' ' +
								 String(_gaugeRadius*.1) + ',' + String(_gaugeRadius*-.1)
								 }"/>
			</degrafa:GeometryGroup>		 
			<degrafa:GeometryGroup id="column06Arrow01">
				<!-- Indicator a Polygon. -->
