			<degrafa:RasterText id="column06Laggers202" fontSize="{_fontSize}" width="{_gaugeRadius}" textColor="#FFFFFF"  fontFamily="Arial" />
			<degrafa:RasterText id="column06Laggers302" fontSize="{_fontSize}" width="{_gaugeRadius}" textColor="#FFFFFF"  fontFamily="Arial" />
			
