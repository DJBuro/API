										gauge02ArrowIndicator.fill = this.greenFill;
									}
									else
									{
										gauge02Arrow.rotation = 0;
										gauge02ArrowDS.rotation = 0;
										gauge02ArrowIndicator.fill = this.redFill;
									}
								}
								else
