							gauge02ArrowDS.x = gauge02Arrow.x +1;
							gauge02ArrowDS.y = gauge02Arrow.y +4;
							
							gauge02LeadersText.y = gauge02Arrow.y + _fontSize*2;
							
							//Leaders -- Laggers
							if(gaugeObject.TopSites != null)
							{ 
								//TopSite
								for (y = 0; y < gaugeObject.TopSites.length; y++)
								{
									switch (y) { 
