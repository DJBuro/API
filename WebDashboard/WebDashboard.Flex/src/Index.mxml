								 data="{
								 String(_gaugeRadius*-.1) + ',' + String(_gaugeRadius*-.1) + ' ' + 
								 String(0) + ',' + String(_gaugeRadius*.1) + ' ' +
								 String(_gaugeRadius*.1) + ',' + String(_gaugeRadius*-.1)
								 }"/>
			</degrafa:GeometryGroup>
			
			<!--Leaders/Laggers-->	
			<degrafa:RasterText id="column06LeadersText01" fontSize="{_fontSize}" text="Leaders"  fontFamily="Arial" fontWeight="bold" textColor="#999999"/>
			<degrafa:RasterText id="column06Leaders101" fontSize="{_fontSize}" width="{_gaugeRadius}" textColor="#FFFFFF"  fontFamily="Arial" />
			<degrafa:RasterText id="column06Leaders201" fontSize="{_fontSize}" width="{_gaugeRadius}" textColor="#FFFFFF"  fontFamily="Arial" />
			<degrafa:RasterText id="column06Leaders301" fontSize="{_fontSize}" width="{_gaugeRadius}" textColor="#FFFFFF"  fontFamily="Arial" />
			<degrafa:RasterText id="column06LaggersText01" fontSize="{_fontSize}" text="Laggers"  fontFamily="Arial" fontWeight="bold" textColor="#999999"/>
			<degrafa:RasterText id="column06Laggers101" fontSize="{_fontSize}" width="{_gaugeRadius}" textColor="#FFFFFF"  fontFamily="Arial" />
			<degrafa:RasterText id="column06Laggers201" fontSize="{_fontSize}" width="{_gaugeRadius}" textColor="#FFFFFF"  fontFamily="Arial" />
			<degrafa:RasterText id="column06Laggers301" fontSize="{_fontSize}" width="{_gaugeRadius}" textColor="#FFFFFF"  fontFamily="Arial" />
			
