		</property>
		<property name="ShortName" type="String">
			<column name="ShortName" length="10" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="CustomDivisorValue" type="Int32">
			<column name="CustomDivisorValue" length="4" sql-type="int" not-null="true"/>
