<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
	<class name="WebDashboard.Dao.Domain.ValueType, WebDashboard.Dao" table="tbl_ValueType">
		<id name="Id" type="Int32" unsaved-value="null">
			<column name="id" length="4" sql-type="int" not-null="true" unique="true" index="PK_tbl_ValueType"/>
			<generator class="native" />
		</id>
		<property name="Name" type="String">
			<column name="Name" length="50" sql-type="nvarchar" not-null="true"/>
		</property>
		<property name="Value" type="String">
			<column name="`Value`" length="50" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="IsPrefix" type="Boolean">
			<column name="isPrefix" length="1" sql-type="bit" not-null="true"/>
		</property>
		<bag name="ValueTypeIdIndicators" inverse="true" lazy="true" cascade="all-delete-orphan">
			<key column="ValueTypeId"/>
			<one-to-many class="WebDashboard.Dao.Domain.Indicator, WebDashboard.Dao"/>
		</bag>
	</class>
</hibernate-mapping>



