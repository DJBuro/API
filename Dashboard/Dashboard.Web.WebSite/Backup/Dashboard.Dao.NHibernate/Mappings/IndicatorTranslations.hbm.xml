<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
	<class name="Dashboard.Dao.Domain.IndicatorTranslation, Dashboard.Dao" table="IndicatorTranslations">
		<id name="Id" type="Int32" unsaved-value="null">
			<column name="IndicatorTranslationsID" length="4" sql-type="int" not-null="true" unique="true" index="PK_IndicatorTranslations"/>
			<generator class="native" />
		</id>
		<property name="TranslatedIndicatorName" type="String">
			<column name="TranslatedIndicatorName" length="100" sql-type="varchar" not-null="true"/>
		</property>
		<many-to-one name="Language" class="Dashboard.Dao.Domain.Language, Dashboard.Dao">
			<column name="LanguageID" length="4" sql-type="int" not-null="true"/>
		</many-to-one>
		<many-to-one name="IndicatorDefinition" class="Dashboard.Dao.Domain.IndicatorDefinition, Dashboard.Dao">
			<column name="IndicatorDefinitionsID" length="4" sql-type="int" not-null="true"/>
		</many-to-one>
	</class>
</hibernate-mapping>



