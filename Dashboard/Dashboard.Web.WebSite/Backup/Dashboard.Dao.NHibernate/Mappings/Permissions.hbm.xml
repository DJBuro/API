<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
	<class name="Dashboard.Dao.Domain.Permission, Dashboard.Dao" table="Permissions">
		<id name="Id" type="Int32" unsaved-value="null">
			<column name="id" length="4" sql-type="int" not-null="true" unique="true" index="PK_Permissions"/>
			<generator class="native" />
		</id>
		<many-to-one name="Site" class="Dashboard.Dao.Domain.Site, Dashboard.Dao">
			<column name="SiteId" length="4" sql-type="int" not-null="true"/>
		</many-to-one>
		<many-to-one name="User" class="Dashboard.Dao.Domain.User, Dashboard.Dao">
			<column name="UserId" length="4" sql-type="int" not-null="true"/>
		</many-to-one>
	</class>
</hibernate-mapping>



