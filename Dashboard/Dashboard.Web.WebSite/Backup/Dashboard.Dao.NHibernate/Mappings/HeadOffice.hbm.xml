<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
	<class name="Dashboard.Dao.Domain.HeadOffice, Dashboard.Dao" table="HeadOffice">
		<id name="Id" type="Int32" unsaved-value="null">
			<column name="HeadOfficeID" length="4" sql-type="int" not-null="true" unique="true" index="PK_HeadOffice"/>
			<generator class="native" />
		</id>
		<property name="HeadOfficeName" type="String">
			<column name="HeadOfficeName" length="100" sql-type="varchar" not-null="true"/>
		</property>
		<many-to-one name="Language" class="Dashboard.Dao.Domain.InterfaceTextLangauge, Dashboard.Dao">
			<column name="LanguageID" length="4" sql-type="int" not-null="true"/>
		</many-to-one>
		<bag name="Regions" inverse="true" lazy="true" cascade="all-delete-orphan">
			<key column="HeadOfficeID"/>
			<one-to-many class="Dashboard.Dao.Domain.Region, Dashboard.Dao"/>
		</bag>
		<bag name="IndicatorDefinitions" inverse="true" lazy="true" cascade="all-delete-orphan">
			<key column="HeadOfficeID"/>
			<one-to-many class="Dashboard.Dao.Domain.IndicatorDefinition, Dashboard.Dao"/>
		</bag>
		<bag name="TradingTimes" inverse="true" lazy="true" cascade="all-delete-orphan">
			<key column="HeadOfficeID"/>
			<one-to-many class="Dashboard.Dao.Domain.TradingTime, Dashboard.Dao"/>
		</bag>
		<bag name="SiteMessages" inverse="true" lazy="true" cascade="all-delete-orphan">
			<key column="HeadOfficeID"/>
			<one-to-many class="Dashboard.Dao.Domain.SiteMessage, Dashboard.Dao"/>
		</bag>
		<bag name="Sites" inverse="true" lazy="true" cascade="all-delete-orphan">
			<key column="HeadOfficeID"/>
			<one-to-many class="Dashboard.Dao.Domain.Site, Dashboard.Dao"/>
		</bag>
	</class>
</hibernate-mapping>



