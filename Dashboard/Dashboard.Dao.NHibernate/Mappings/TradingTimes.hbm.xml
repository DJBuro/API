<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
	<class name="Dashboard.Dao.Domain.TradingTime, Dashboard.Dao" table="TradingTimes">
		<id name="Id" type="Int32" unsaved-value="null">
			<column name="TradingTimesID" length="4" sql-type="int" not-null="true" unique="true" index="PK_TradingTimes"/>
			<generator class="native" />
		</id>
		<property name="WeekDayNumber" type="Int32">
			<column name="WeekDayNumber" length="4" sql-type="int" not-null="true"/>
		</property>
		<property name="OpeningTime" type="String">
			<column name="OpeningTime" length="10" sql-type="varchar" not-null="true"/>
		</property>
		<property name="ClosingTime" type="String">
			<column name="ClosingTime" length="50" sql-type="varchar" not-null="true"/>
		</property>
		<many-to-one name="HeadOffice" class="Dashboard.Dao.Domain.HeadOffice, Dashboard.Dao">
			<column name="HeadOfficeID" length="4" sql-type="int" not-null="true"/>
		</many-to-one>
	</class>
</hibernate-mapping>



