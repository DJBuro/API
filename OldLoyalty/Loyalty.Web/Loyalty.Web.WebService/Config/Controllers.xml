<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net">

    <object name="LoyaltyCardService" type="Loyalty.Web.Service.LoyaltyCardService, Loyalty.Web.Service">
        <!--<property name="Message">
            <value>Not used... but could be...</value>
        </property>-->
        <property name="LoyaltyCardDao" ref="LoyaltyCardDao" />
        <property name="LoyaltyUserDao" ref="LoyaltyUserDao" />
        <property name="RamesesAddressDao" ref="RamesesAddressDao" />
        <property name="SiteDao" ref="SiteDao" />
        <property name="CompanyDao" ref="CompanyDao" />
        <property name="StatusDao" ref="StatusDao" />
    </object>

    <object id="LoyaltyCardWebService" type="Spring.Web.Services.WebServiceExporter, Spring.Web">
        <property name="TargetName" value="LoyaltyCardService"/>
        <property name="Namespace" value="http://www.Andromeda.co.uk/LoyaltyCard"/>
        <property name="Description" value="Defines operations upon products"/>
        <property name="MemberAttributes">
            <dictionary>
                <entry key="AddPointsToLoyaltyCard">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Adds points to loyalty card"/>
                        <property name="MessageName" value="AddPointsToLoyaltyCard"/>
                    </object>
                </entry>
                <entry key="AddUpdateLoyaltySite">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Adds or updates a loyalty site"/>
                        <property name="MessageName" value="AddUpdateLoyaltySite"/>
                    </object>
                </entry>
                <entry key="FindLoyaltyCardByAddress">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Gets loyalty cards by an address"/>
                        <property name="MessageName" value="FindLoyaltyCardByAddress"/>
                    </object>
                </entry>
                <entry key="GetLoyaltyCardDetails">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Gets loyalty card details"/>
                        <property name="MessageName" value="GetLoyaltyCardDetails"/>
                    </object>
                </entry>
                <entry key="GetLoyaltyCardPoints">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Gets available loyalty card points"/>
                        <property name="MessageName" value="GetLoyaltyCardPoints"/>
                    </object>
                </entry>
                <entry key="GetRatios">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Gets the points required to redeem/gain 100 cents/pence, and rounding business rules, null on error - check GetLastTwentyLogs"/>
                        <property name="MessageName" value="GetRatios"/>
                    </object>
                </entry>
                <entry key="RemovePointsFromLoyaltyCard">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Removes points from a loyalty card"/>
                        <property name="MessageName" value="RemovePointsFromLoyaltyCard"/>
                    </object>
                </entry>
                <!--<entry key="FindLoyaltyCardById">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Delete... test class"/>
                        <property name="MessageName" value="FindLoyaltyCardById"/>
                    </object>
                </entry>-->
                

            </dictionary>
        </property>
    </object>


    <object name="LoyaltyCardSiteService" type="Loyalty.Web.Service.LoyaltyCardSiteService, Loyalty.Web.Service">
        <property name="LoyaltyCardDao" ref="LoyaltyCardDao" />
        <property name="LoyaltyAccountDao" ref="LoyaltyAccountDao" />
        <property name="LoyaltyUserDao" ref="LoyaltyUserDao" />
        <property name="RamesesAddressLoyaltyCardDao" ref="RamesesAddressLoyaltyCardDao" />
        <property name="RamesesAddressDao" ref="RamesesAddressDao" />
        <property name="AccountAddressDao" ref="AccountAddressDao" />
        <property name="SiteDao" ref="SiteDao" />
        <property name="UserTitleDao" ref="UserTitleDao" />
        <property name="CompanyUserTitleDao" ref="CompanyUserTitleDao" />
        <property name="CountryDao" ref="CountryDao" />
        <property name="AccountStatusDao" ref="AccountStatusDao" />
        <property name="LoyaltyAccountStatusDao" ref="LoyaltyAccountStatusDao" />
        <property name="StatusDao" ref="StatusDao" />
        <property name="LoyaltyCardStatusDao" ref="LoyaltyCardStatusDao" />
        <property name="LoyaltyLogDao" ref="LoyaltyLogDao" />
        <property name="OrderTypeDao" ref="OrderTypeDao" />
    </object>

    <object id="LoyaltyCardSiteWebService" type="Spring.Web.Services.WebServiceExporter, Spring.Web">
        <property name="TargetName" value="LoyaltyCardSiteService"/>
        <property name="Namespace" value="http://www.Andromeda.co.uk/LoyaltyCardSite"/>
        <property name="Description" value="Defines operations upon products"/>
        <property name="MemberAttributes">
            <dictionary>
                <entry key="GetRatios">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Gets the points required to redeem/gain 100 cents/pence, and rounding business rules, null on error - check GetLastTwentyLogs"/>
                        <property name="MessageName" value="GetRatios"/>
                    </object>
                </entry>
                <entry key="GetLastTwentyLogs">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Gets the last 20 logs that your site has generated, loyaltyLog.message on error"/>
                        <property name="MessageName" value="GetLastTwentyLogs"/>
                    </object>
                </entry>
                <entry key="FindAssociatedAddress">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Gets addresses that a loyalty card has been used at, null on error - check GetLastTwentyLogs"/>
                        <property name="MessageName" value="FindAssociatedAddress"/>
                    </object>
                </entry>
                <entry key="RemoveAssociatedAddress">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Removes an address that a loyalty card has been used at, check Message.error for error"/>
                        <property name="MessageName" value="RemoveAssociatedAddress"/>
                    </object>
                </entry>
                <entry key="ApplyLoyaltyCard">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Applies the methods AddPointsToLoyaltyCard and RemovePointsFromLoyaltyCard, but only using the transaction history to determine amounts, check Message.error for error"/>
                        <property name="MessageName" value="ApplyLoyaltyCard"/>
                    </object>
                </entry>
                <entry key="AddPointsToLoyaltyCard">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Adds points to a loyalty account via loyalty card, only to be used in special cases, check Message.error for error"/>
                        <property name="MessageName" value="AddPointsToLoyaltyCard"/>
                    </object>
                </entry>
                <entry key="RemovePointsFromLoyaltyCard">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Removes points from a loyalty account via loyalty card, only to be used in special cases, check Message.error for error"/>
                        <property name="MessageName" value="RemovePointsFromLoyaltyCard"/>
                    </object>
                </entry>
                <entry key="GetLoyaltyCardPoints">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Gets available loyalty card points, -1 on error - check GetLastTwentyLogs"/>
                        <property name="MessageName" value="GetLoyaltyCardPoints"/>
                    </object>
                </entry>
                <entry key="GetOrderTypes">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Gets available order types, delivery, collection etc."/>
                        <property name="MessageName" value="GetOrderTypes"/>
                    </object>
                </entry>
                <entry key="GetOrderTypeById">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Gets an order type by id"/>
                        <property name="MessageName" value="GetOrderTypeById"/>
                    </object>
                </entry>
                <entry key="ViewLoyaltyCardPin">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Returns the loyalty card pin, or returns error"/>
                        <property name="MessageName" value="ViewLoyaltyCardPin"/>
                    </object>
                </entry>
                <entry key="ChangeLoyaltyCardPin">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Changes the loyalty card pin, if loyalty card has one, check Message.error for error"/>
                        <property name="MessageName" value="ChangeLoyaltyCardPin"/>
                    </object>
                </entry>
                <entry key="AddUpdateLoyaltyCardPin">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Adds a loyalty card pin, if loyalty card does not already have one, check Message.error for error"/>
                        <property name="MessageName" value="AddUpdateLoyaltyCardPin"/>
                    </object>
                </entry>
                <entry key="StolenLoyaltyCard">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Report a loyalty card stolen and make it inactive, check Message.error for error"/>
                        <property name="MessageName" value="StolenLoyaltyCard"/>
                    </object>
                </entry>
                <entry key="LostLoyaltyCard">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Report a loyalty card lost and make it inactive, check Message.error for error"/>
                        <property name="MessageName" value="LostLoyaltyCard"/>
                    </object>
                </entry>
                <entry key="RemoveLoyaltyCard">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Removes and deactivates a loyalty card from an account, check Message.error for error"/>
                        <property name="MessageName" value="RemoveLoyaltyCard"/>
                    </object>
                </entry>
                <entry key="AddLoyaltyCardToAccount">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Add an unregistered loyalty card to an account and tranfers points, check Message.error for error"/>
                        <property name="MessageName" value="AddLoyaltyCardToAccount"/>
                    </object>
                </entry>
                <entry key="GetForgottenPassword">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Returns the loyaty account password from an email address, null if no email address found - check GetLastTwentyLogs"/>
                        <property name="MessageName" value="GetForgottenPassword"/>
                    </object>
                </entry>
                <entry key="GetLoyaltyAccountById">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Get an account and user details by an accountId, null on error - check GetLastTwentyLogs"/>
                        <property name="MessageName" value="GetLoyaltyAccountById"/>
                    </object>
                </entry>
                <entry key="GetLoyaltyAccount">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Gets an account and user details if they have registered, null on error - check GetLastTwentyLogs"/>
                        <property name="MessageName" value="GetLoyaltyAccount"/>
                    </object>
                </entry>
                <entry key="GetUserTitles">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Gets available user titles, no errors returned"/>
                        <property name="MessageName" value="GetUserTitles"/>
                    </object>
                </entry>
                <entry key="GetCountries">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Gets available countries, no errors returned"/>
                        <property name="MessageName" value="GetCountries"/>
                    </object>
                </entry>                
                <entry key="UpdateUser">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Updates a registered users details, check Message.error for error"/>
                        <property name="MessageName" value="UpdateUser"/>
                    </object>
                </entry>
                <entry key="UpdateLoyaltyAccountAddress">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Updates an account address, check Message.error for error"/>
                        <property name="MessageName" value="UpdateLoyaltyAccountAddress"/>
                    </object>
                </entry>
                <entry key="Register">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Registering a loyalty card account, a card, a user, and an address using one class, returns a Message.error on error - returns Message.success with loyalty account id on success"/>
                        <property name="MessageName" value="Register"/>
                    </object>
                </entry>
                <entry key="GetTransactionHistory">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="Gets the transaction history of a loyalty card, null on error - check GetLastTwentyLogs"/>
                        <property name="MessageName" value="GetTransactionHistory"/>
                    </object>
                </entry>
                <!--<entry key="FindLoyaltyCardById">
                    <object type="System.Web.Services.WebMethodAttribute, System.Web.Services">
                        <property name="Description" value="TestClass -  ignore"/>
                        <property name="MessageName" value="FindLoyaltyCardById"/>
                    </object>
                </entry>-->
            </dictionary>
        </property>
    </object>


    
</objects>
