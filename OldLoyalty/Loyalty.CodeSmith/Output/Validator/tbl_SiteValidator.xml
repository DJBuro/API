
<objects xmlns="http://www.springframework.net">

<v:group id="TblSiteValidator" xmlns:v="http://www.springframework.net/validation">


	<v:required id="NameValidator" test="Name"><v:message id="error.Name.required" providers="NameErrors, ValidationSummary"/></v:required>
	<v:required id="SiteKeyValidator" test="SiteKey"><v:message id="error.SiteKey.required" providers="SiteKeyErrors, ValidationSummary"/></v:required>
	<v:required id="SitePasswordValidator" test="SitePassword"><v:message id="error.SitePassword.required" providers="SitePasswordErrors, ValidationSummary"/></v:required>
</v:group>

</objects>




			