
<objects xmlns="http://www.springframework.net">

<v:group id="OrderItemHistoryValidator" xmlns:v="http://www.springframework.net/validation">


	<v:required id="NameValidator" test="Name"><v:message id="error.Name.required" providers="NameErrors, ValidationSummary"/></v:required>
	<v:required id="ItemPriceValidator" test="ItemPrice"><v:message id="error.ItemPrice.required" providers="ItemPriceErrors, ValidationSummary"/></v:required>
	<v:required id="ItemLoyaltyPointsValidator" test="ItemLoyaltyPoints"><v:message id="error.ItemLoyaltyPoints.required" providers="ItemLoyaltyPointsErrors, ValidationSummary"/></v:required>
</v:group>

</objects>




			