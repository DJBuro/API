<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
	<class name="Loyalty.Dao.Domain.OrderItemHistory, Loyalty.Dao" table="tbl_OrderItemHistory">
		<id name="Id" type="Int32" unsaved-value="null">
			<column name="id" length="4" sql-type="int" not-null="true" unique="true" index="PK_tbl_OrderItemHistory"/>
			<generator class="native" />
		</id>
		<property name="Name" type="String">
			<column name="Name" length="50" sql-type="nvarchar" not-null="true"/>
		</property>
		<property name="ItemPrice" type="Decimal">
			<column name="ItemPrice" length="5" sql-type="decimal" not-null="true"/>
		</property>
		<property name="ItemLoyaltyPoints" type="Int32">
			<column name="ItemLoyaltyPoints" length="4" sql-type="int" not-null="true"/>
		</property>
		<many-to-one name="TransactionHistoryId" class="Loyalty.Dao.Domain.TransactionHistory, Loyalty.Dao">
			<column name="TransactionHistoryId" length="4" sql-type="int" not-null="true"/>
		</many-to-one>
	</class>
</hibernate-mapping>



