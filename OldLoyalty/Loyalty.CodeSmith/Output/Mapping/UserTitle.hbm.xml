<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
	<class name="Loyalty.Dao.Domain.UserTitle, Loyalty.Dao" table="tbl_UserTitle">
		<id name="Id" type="Int32" unsaved-value="null">
			<column name="id" length="4" sql-type="int" not-null="true" unique="true" index="PK_tbl_UserTitle"/>
			<generator class="native" />
		</id>
		<property name="Title" type="String">
			<column name="Title" length="5" sql-type="nvarchar" not-null="true"/>
		</property>
		<bag name="UserTitleIdLoyaltyUsers" inverse="true" lazy="true" cascade="all-delete-orphan">
			<key column="UserTitleId"/>
			<one-to-many class="Loyalty.Dao.Domain.LoyaltyUser, Loyalty.Dao"/>
		</bag>
		<bag name="UserTitleIdCompanyUserTitleses" inverse="true" lazy="true" cascade="all-delete-orphan">
			<key column="UserTitleId"/>
			<one-to-many class="Loyalty.Dao.Domain.CompanyUserTitle, Loyalty.Dao"/>
		</bag>
	</class>
</hibernate-mapping>



